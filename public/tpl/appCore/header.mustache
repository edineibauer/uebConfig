<div id="core-header-container">
    <header id="core-header-logo">
        <a href="{{home}}{{homepage}}" id="logo-href">
            <img src='{{home}}assetsPublic/img/favicon-48.png' height='35' style='height: 35px;padding-right:5px' class='core-header-img' />
            <h1 id='core-header-title' class='theme-text-aux'>{{sitename}}</h1>
        </a>
    </header>

    <nav role="navigation">
        <ul id="core-header-nav">
            <div id="core-menu-custom">
                {{#menu}}
                    <li class="menu-li {{li.class}}"  {{#li.style}}style="{{{li.style}}}" {{/li.style}}{{{li.attr}}} >
                        {{#funcao}}
                            <div onclick="{{funcao}}()" class="{{class}}" {{#style}}style="{{{style}}}"{{/style}} {{{attr}}}>{{{html}}}</div>
                        {{/funcao}}
                        {{#href}}
                            <a href="{{href}}" class="{{class}}" {{#style}}style="{{{style}}}" {{/style}}{{{attr}}}>{{{html}}}</a>
                        {{/href}}
                        {{^funcao}}
                            {{^href}}
                                <div class="{{class}}" {{#style}}style="{{{style}}}"{{/style}} {{{attr}}}>{{{html}}}</div>
                            {{/href}}
                        {{/funcao}}
                    </li>
                {{/menu}}
            </div>
        </ul>
    </nav>
</div>